<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="416.0" prefWidth="309.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.LoginController">
   <children>
      <TextField fx:id="usernameTextField" layoutX="80.0" layoutY="142.0" onAction="#loginErrorLabelOnAction" promptText="Enter Email" />
      <Text layoutX="180.0" layoutY="167.0" strokeType="OUTSIDE" strokeWidth="0.0" />
      <Text layoutX="125.0" layoutY="86.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Login:">
         <font>
            <Font size="22.0" />
         </font></Text>
      <Text layoutX="80.0" layoutY="183.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password:" />
      <PasswordField fx:id="passwordTextField" layoutX="80.0" layoutY="187.0" onAction="#loginErrorLabelOnAction" promptText="Enter Password" />
      <Button fx:id="signUpButton" layoutX="116.0" layoutY="313.0" mnemonicParsing="false" onAction="#signUpButtonOnAction" text="Create Account" />
      <Button fx:id="loginButton" layoutX="142.0" layoutY="263.0" mnemonicParsing="false" onAction="#loginButtonOnAction" text="Login" />
      <Button fx:id="HelpButton" layoutX="49.0" layoutY="333.0" mnemonicParsing="false" onAction="#HelpButtonOnAction" text="Help" />
      <Button fx:id="LCancelButton" layoutX="222.0" layoutY="333.0" mnemonicParsing="false" onAction="#LCancelButtonOnAction" text="Cancel" />
      <Text layoutX="80.0" layoutY="138.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Email:" />
      <Button fx:id="ForgotPasswordButton" layoutX="138.0" layoutY="212.0" mnemonicParsing="false" onAction="#forgotPasswordButtonOnAction" text="Forget Password?">
         <font>
            <Font name="Palatino Linotype" size="10.0" />
         </font>
      </Button>
      <Label fx:id="loginErrorLabel" layoutX="80.0" layoutY="240.0" style="-fx-font-size: 12px; -fx-font-family: 'Palatino Linotype';" text="" textFill="red" />
      <Button fx:id="googleLoginButton" layoutX="50.0" layoutY="288.0" mnemonicParsing="false" onAction="#googleLoginButtonOnAction" text="Login with Google" />
      <Button fx:id="facebookLoginButton" layoutX="159.0" layoutY="288.0" mnemonicParsing="false" onAction="#facebookLoginButtonOnAction" text="Login with Facebook" />
   </children>
</AnchorPane>
